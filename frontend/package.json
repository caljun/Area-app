    "@rnmapbox/maps": "^10.0.23",
    "axios": "^1.10.0",
    "expo": "~51.0.28",
    "expo-camera": "~14.0.5",
    "expo-constants": "~16.0.2",
    "expo-file-system": "~18.0.12",
    "expo-font": "~12.0.10",
    "expo-image-picker": "~15.0.3",
    "expo-keep-awake": "~13.0.1",
    "expo-linking": "~6.2.2",
